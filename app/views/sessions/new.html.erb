<h1>Sign In</h1>
<%= form_tag sessions_url do %>
	<div class="field">
		<%= label_tag :email %>
		<%= email_field_tag :email, params[:email] %>
	</div>
	<div class="field">
		<%= label_tag :password %>
		<%= password_field_tag :password %>
	</div>
	<div class="field">
		<%= submit_tag "Log In" %>
	<% end %>
